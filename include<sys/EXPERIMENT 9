                       exp9.c                                   
#include<sys/wait.h>
int main()
{
pid_t p;
printf("Before fork\n");
p=fork();
if(p==0)
{
printf("I am the child process having ID %d\n",getpid() );
printf("My parent ID is %d\n",getppid() );
}
else
{
wait(NULL);
printf("My child process ID is %d\n",p);
printf("I am Parent having ID %d \n",getpid() );
}
printf("Common \n");
}
